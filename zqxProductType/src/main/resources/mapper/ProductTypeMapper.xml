<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zqx.producttype.mapper.ProductTypeMapper">
    <insert id="saveProductType" parameterType="com.zqx.commom.entity.ProductTypeInfo">
        <selectKey keyProperty="id" order="AFTER" resultType="java.lang.Integer">
            SELECT LAST_INSERT_ID()
        </selectKey>
        insert into productType(producttypeName,producttypeDescription,parentId,grade)
        value(#{producttypeName},#{producttypeDescription},#{parentId},#{grade})
    </insert>

</mapper>