<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zqx.product.mapper.ProductDetailMapper">
    <insert id="saveProductDetail" parameterType="com.zqx.commom.entity.ProductDetail">
        insert into productdetail(productId,productPlace,productBrand,productDescription,productWeight,productDetailPicUrl,specsAndPack)
        value(#{productId},#{productPlace},#{productBrand},#{productDescription},#{productWeight},#{productDetailPicUrl},#{specsAndPack})
    </insert>

</mapper>